{
 "compilerOptions": {
  "target": "es5",
  "module": "commonjs",
  "lib": [
   "es2017",
   "es7",
   "es6",
   "dom"
  ],
  "declaration": true,
  "outDir": "dist",
  "strict": true,
  "esModuleInterop": true
 },
 "exclude": [
  "node_modules",
  "dist"
 ],
 "typedocOptions": {
  "entryPoints": [
  "index.ts"
  ],
  "out": "docs",
  "plugin": ["typedoc-material-theme"],
  "themeColor": "#c27ba0",
  "name": "Any API - v2.0",
 }
}
